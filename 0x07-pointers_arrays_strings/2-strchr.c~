#include "main.h"

/**
 * _strch - locate character in string
 * @s: source string
 * @c: character to find
 *
 * Return: the string from character found
 */
char *_strchr(char *s, char c)
{
int a =0; b;

while ([a])
{
a++;
}

for (a = 0; b <= a; b++)
{
if (c == s[b])
{
s += b;
return (s);
}
}

return ('\0');
}
